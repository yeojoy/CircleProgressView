<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="style" format="enum">
        <enum name="stroke" value="0" />
        <enum name="fill" value="1" />
    </attr>
    <declare-styleable name="CircleProgressView">
        <attr name="style" />
        <attr name="line_width" format="dimension" />
        <attr name="background_line_width" format="dimension" />
        <attr name="line_color" format="color" />
        <attr name="background_line_color" format="color" />
        <attr name="value" format="integer" />
        <attr name="has_shadow" format="boolean" />
        <attr name="has_animation" format="boolean" />
        <attr name="animation_duration" format="integer" />
    </declare-styleable>
</resources>